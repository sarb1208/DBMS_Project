                  <InnerShadow />
               </effect>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></PasswordField>
            <Button fx:id="signupButton" layoutX="48.0" layoutY="454.0" mnemonicParsing="false" onAction="#signup" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: #FFFFFF;" text="Sign Up">
