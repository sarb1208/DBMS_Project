               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <PasswordField fx:id="repassword" layoutX="48.0" layoutY="286.0" prefHeight="26.0" prefWidth="273.0" promptText="Re-type Password">
