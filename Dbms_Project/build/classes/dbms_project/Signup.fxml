            </Label>
            <TextField fx:id="mobileNo" layoutX="48.0" layoutY="179.0" prefHeight="26.0" prefWidth="273.0" promptText="MobileNo">
               <effect>
                  <InnerShadow />
               </effect>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <TextField fx:id="Name" layoutX="48.0" layoutY="75.0" prefHeight="26.0" prefWidth="273.0" promptText="Name">
