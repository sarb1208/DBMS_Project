                  <InnerShadow />
               </effect>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextArea>
         </children>
         <effect>
            <InnerShadow />
         </effect>
      </AnchorPane>
      <ImageView fx:id="logo" fitHeight="247.0" fitWidth="323.0" layoutX="27.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true">
