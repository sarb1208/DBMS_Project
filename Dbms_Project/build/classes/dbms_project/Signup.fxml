                  <InnerShadow />
               </effect>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <PasswordField fx:id="password" layoutX="48.0" layoutY="234.0" prefHeight="26.0" prefWidth="273.0" promptText="Password">
