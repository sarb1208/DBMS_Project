               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></PasswordField>
            <TextArea fx:id="addressField" layoutX="48.0" layoutY="321.0" prefHeight="117.0" prefWidth="273.0" promptText="Address">
